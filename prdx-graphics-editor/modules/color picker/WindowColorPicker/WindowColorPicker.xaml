<Window x:Class="prdx_graphics_editor.modules.color_picker.WindowColorPicker.WindowColorPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prdx_graphics_editor.modules.color_picker.WindowColorPicker"
        mc:Ignorable="d"
        Title="Выбор цвета" Height="300" Width="400">
    <Grid>
        <Frame x:Name="ColorPickerFrame" VerticalAlignment="Top" Height="200" HorizontalAlignment="Stretch" Source="..\FormColorPicker\PageColorPicker.xaml"/>
        <Button x:Name="ButtonApply" Width="75" Height="25" Margin="227,234,90,10" Click="ButtonApply_Click">Применить</Button>
        <Button x:Name="ButtonCancel" Width="75" Height="25" Margin="307,234,10,10" Click="ButtonCancel_Click">Отмена</Button>
    </Grid>
</Window>
